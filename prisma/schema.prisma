datasource db {
  provider = "sqlite"
  url      = "file:./database.db"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  username String @id @unique
  password String
}

model GameSession {
  username String @id @unique
  progress Int @default(0)
  double Int @default(0)
  shield Int @default(0)
  half Int @default(0)
  status Int @default(0)
  endTime DateTime
}